%description:
Test VehicleIdentification class

%includes:
#include <string>
#include "inet/networklayer/ipv6mev/VehicleIdentification.h"

%global:
using namespace inet;

%activity:
EV << "Starting test:" << endl;
VehicleIdentification *vid = new VehicleIdentification();
EV << vid->get_str() << endl;
delete vid;
vid = new VehicleIdentification(123456789);
EV << vid->get_str() << endl;
vid->setId(987654321);
EV << vid->get_str() << endl;
vid->setId("DEADAFFEDEADAFFE");
EV << vid->get_str() << endl;
vid->setId("DEADAFFE:DEADAFFE");
EV << vid->get_str() << endl;
EV << vid->isUnspecified() << endl;
delete vid;
vid = new VehicleIdentification(1, 1234);
EV << vid->get_str() << endl;
EV << vid->isUnicast() << endl;
EV << vid->isUnspecified() << endl;
vid->setId(1229801703532086340);
EV << vid->get_str() << endl;
EV << vid->str() << endl; // 11112222:33334444
EV << vid->tryParse(vid->str().c_str()) << endl;
vid->setId("BABEBABE:BABEBABE");
EV << vid->str() << endl; // BABEBABE:BABEBABE
EV << vid->tryParse(0) << endl;
EV << vid->tryParse("BABEBABE:BABEBABEA") << endl;
EV << vid->tryParse("BABEBABEBABEBABE") << endl;

delete vid;

